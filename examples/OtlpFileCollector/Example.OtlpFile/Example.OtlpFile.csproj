  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\..\src\Essential.OpenTelemetry.Exporter.OtlpFile\Essential.OpenTelemetry.Exporter.OtlpFile.csproj" />
  </ItemGroup>
